{% block extends %}
{{ "{% extends 'base.html.twig' %}" }}
{% endblock extends %}
{{ "{% block breadcrumb %}" }}
    <a href="{{ "{{ path('" ~ route_name_prefix ~ "_index') }}" }}">
       {{ "{{ '"~ entity|lower~".list.name'|trans }}" }}
    </a>
{{ "{% endblock %}" }}
	{{ " {% block breadcrumbActive %}{{ '"~ entity|lower~".edit.name'|trans }}{% endblock %}" }}
{% block body %}
{{ "{% block body %}" }}
<div class="col-sm-6">
    <div data-widget-group="group1">
        <div class="panel panel-default" data-widget='{"draggable": "false"}'>
            <div class="panel-heading">
                <h2><i class="ti ti-pencil-alt"></i> {{"{{ 'journalconsumable.edit.name'|trans }}"}}</h2>
                <div class="panel-ctrls" data-actions-container="" data-action-collapse='{"target": ".panel-body"}'></div>
            </div>
            <div class="panel-editbox" data-widget-controls=""></div>
            <div class="panel-body">
                {{ "{{ form_start(formDetail,{
                        'attr' : { 'class' : 'form-horizontal row-border'  } 
                    }) }}"}}

                {{ "{{ form_widget(formDetail) }}"}}
                    <div class="form-group">
                        <div class="col-sm-8 col-sm-offset-2">
                        <div class="pull-left">
                            <button class="finish btn btn-primary-alt " type="submit">
                            <i class="ti ti-pencil-alt"></i>
                            {{"{{ 'supportingdocument.edit.submit'|trans }}"}}
                        </button>
                        </div>
                        <div class="pull-rigth">
                            <a href="{{"{{ path('" ~ route_name_prefix ~ "_new') }}"}}" class="finish btn btn-danger-alt ">
                            <i class="ti ti-annuler"></i>
                            {{ "{{ 'supportingdocument.edit.annuler'|trans }}"}}
                            </a>
                        </div>
                        </div>
                    </div>
                {{ "{{ form_end(formDetail) }}"}}
            </div>
            <div class="panel-footer"></div>
        </div>
    </div>
</div>
    <div class="col-sm-6">
        <div data-widget-group="group1">
            <div class="panel panel-default" data-widget='{"draggable": "false"}'>
                <div class="panel-heading">
                    <h2><i class="ti ti-pencil-alt"></i>{{" {{ 'supportingdocument.list.name'|trans }} "}}</h2>
                    <div class="panel-ctrls" data-actions-container="" data-action-collapse='{"target": ".panel-body"}'></div>
                </div>
                <div class="panel-editbox" data-widget-controls=""></div>
                <div class="panel-body">
                <table id="example" class="table table-striped table-bordered" cellspacing="0" width="100%">
                    <thead>
                        <tr>
                            <th>{{ "{{ 'magazine.field.designation'|trans }}"}}</th>
                            <th>{{ "{{ 'magazine.field.quantity'|trans }}"}}</th>
                            <th>{{ "{{ 'magazine.action'|trans }}"}}</th>
                        </tr>
                    </thead>
                    <tbody>
                        {{ " {% if details|length == 0 %} "}}
                            <td colspan="5">
                                <div class="well well-lg tooltips" data-trigger="hover" data-original-title=".well.well-sm">
                                    <i class="ti ti-alert"></i>{{"{{ 'supportingdocument.list.noResultFound'|trans }} "}}
                                </div>
                             </td>
                        {{ "{% else %} "}}
                        {{"{% for detail in details %}"}}
                            <tr>
                                <td>{{ "{{ detail.article.designation }}"}}</td>
                                <td>{{ "{{ detail.quantity }}"}}</td> 
                                <td>
                                    
                                    <div class="btn-group">
                                        <a href="{{ "{{ path('" ~ route_name_prefix ~ "_edit', { 'id': " ~ entity_singularized ~ "."~ identifier ~ " , 'idDetail' : detail.id}) }}"}}" class="btn btn-info-alt"><i class="ti ti-pencil-alt"></i></a>
                                        <a href="#" data-box="#{{ "{{ detail.article.id }}"}}" class="btn btn-danger-alt mb-control"><i class="ti ti-trash"></i></a>
                                    </div>
                                </td>                                    <!-- MESSAGE BOX-->
                                    <div class="message-box animated fadeIn" data-sound="alert" id="{{ "{{ detail.article.id }}"}}">
                                        <div class="mb-container">
                                            <div class="mb-middle">
                                                <div class="mb-title"><span class="ti ti-trash"></span>{{" {{ 'supportingdocument.delete.name'|trans }} "}}?</div>
                                                <div class="mb-content">
                                                    <p>{{"{{ 'supportingdocument.delete.message'|trans }}"}}</p>                    
                                                    
                                                </div>
                                                <div class="mb-footer">
                                                    <div class="pull-right">
                                                        <form action="{{ "{{ path('" ~ route_name_prefix ~ "detail_delete', { 'idDetail': detail.id , 'idArticle' : " ~ entity_singularized ~ "."~ identifier ~ "}) }} "}} " method="post">
                                                            <input type="hidden" name="_method" value="DELETE" />
                                                            {{"{{ form_widget(deleteForms[detail.id]) }} "}}
                                                            <button type="submit" class="btn btn-danger btn-lg">
                                                                <i class="icon-trash"></i>
                                                                {{ " {{ 'messageBox.yes'|trans }} "}}
                                                            </button>
                                                        </form>
                                                        <button class="btn btn-default btn-lg mb-control-close">
                                                           {{ " {{ 'messageBox.no'|trans }} "}}</button>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                <!-- END MESSAGE BOX-->
                            </tr>
                        {{ "{% endfor %}"}}
                    {{ "{% endif %}"}}
                    </tbody>
                </table>
            </div>
            <div class="panel-footer"></div>
        </div>
    </div>
</div>
<div class="col-sm-12">
    <div data-widget-group="group1">
        <div class="panel panel-default" data-widget='{"draggable": "false"}'>
            <div class="panel-heading">
                <h2><i class="ti ti-pencil-alt"></i> {{"{{ '"~ entity|lower~".edit.name'|trans }}"}}</h2>
                <div class="panel-ctrls" data-actions-container="" data-action-collapse='{"target": ".panel-body"}'></div>
            </div>
            <div class="panel-editbox" data-widget-controls=""></div>
            <div class="panel-body">
    	    {{ "{{ form_start(edit_form ,{
                            'attr' : { 'class' : 'form-horizontal row-border' } 
                        }) }}" }}

    	        {{ '{{ form_widget(edit_form) }}' }}
    	    		<div class="form-group">
                        <div class="col-sm-8 col-sm-offset-2">
                        <button class="btn btn-primary " type="submit">
                            <i class="ti ti-pencil-alt"></i>
                            {{"{{ '"~ entity|lower~".edit.submit'|trans }}"}}
                        </button>
                        </div>
                    </div>
                {{ '{{ form_end(edit_form) }}' }}
            </div>
            <div class="panel-footer"></div>
        </div>
    </div>
</div>
{{ "{% endblock %}" }}
{% endblock body %}
